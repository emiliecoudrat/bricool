<h1>
  <div class="text-center">
    NOS BRICOOLERS
  </div>
</h1>

<div class="container">
  <% if policy(@bricooler).index? %>
    <% @bricoolers.each do |bricooler| %>
      <div class="row well">
        <div class="col-sm-3">
          <%= bricooler.first_name %>
          <%= bricooler.last_name %>
          <%= bricooler.bio %>
        </div>
      </div>
    <% end %>
  <% end %>
</div>
